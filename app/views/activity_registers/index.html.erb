<p id="notice"><%= notice %></p>

<h1>Activity Registers</h1>

<table>
  <thead>
    <tr>
      <th>Activity</th>
      <th>User</th>
      <th>Picture1</th>
      <th>Picture2</th>
      <th>Picture3</th>
      <th>Comments</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_registers.each do |activity_register| %>
      <tr>
        <td><%= activity_register.activity %></td>
        <td><%= activity_register.user_id %></td>
        <td><%= activity_register.picture1 %></td>
        <td><%= activity_register.picture2 %></td>
        <td><%= activity_register.picture3 %></td>
        <td><%= activity_register.comments %></td>
        <td><%= activity_register.value %></td>
        <td><%= link_to 'Show', activity_register %></td>
        <td><%= link_to 'Edit', edit_activity_register_path(activity_register) %></td>
        <td><%= link_to 'Destroy', activity_register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Register', new_activity_register_path %>
